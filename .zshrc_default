
# Execute commands without keeping them in history:
setopt HIST_IGNORE_SPACE


# Aliases:
alias ll="ls -lah"
alias c="clear"
alias grep="grep --color=auto"
alias h="history"

# Apps aliases:
alias st="open -a 'Sublime Text' ./"
alias pc="open -a 'Pycharm CE' ./"
alias ij="open -a 'IntelliJ IDEA CE' ./"

# Git aliases:
alias gs="git status"
alias gg="git log --oneline --graph --all"
